{
  "name": "Pantheon",
  "version": "0.0.1-alpha.1",
  "description": "An Electron application with Vue and TypeScript",
  "main": "./out/main/index.js",
  "author": "Zephyruso",
  "type": "module",
  "homepage": "https://electron-vite.org",
  "build": {
    "appId": "com.pantheon.app",
    "productName": "Pantheon"
  },
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint --cache .",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "prepare": "node scripts/build-helper.js",
    "build": "npm run prepare && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "npm run build && electron-builder --mac",
    "build:linux": "npm run build && electron-builder --linux",
    "updateui": "cd src/renderer && git pull"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^4.0.0",
    "@eslint/plugin-kit": "^0.3.2",
    "@fontsource/fira-sans": "^5.2.6",
    "@guolao/vue-monaco-editor": "^1.5.5",
    "@heroicons/vue": "^2.2.0",
    "@tanstack/vue-table": "^8.21.3",
    "@tanstack/vue-virtual": "^3.13.10",
    "@types/reconnectingwebsocket": "^1.0.10",
    "@vueuse/core": "^13.3.0",
    "auto-launch": "^5.0.6",
    "axios": "^1.9.0",
    "countup.js": "^2.9.0",
    "daisyui": "^5.0.43",
    "dayjs": "^1.11.13",
    "dompurify": "^3.2.6",
    "echarts": "^5.6.0",
    "electron-store": "^10.0.1",
    "electron-updater": "^6.3.9",
    "ipaddr.js": "^2.2.0",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "misans": "^4.1.0",
    "p-limit": "^6.2.0",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-tailwindcss": "^0.6.12",
    "pretty-bytes": "^7.0.0",
    "reconnectingwebsocket": "^1.0.0",
    "socket.io-client": "^4.8.1",
    "sort-package-json": "^3.2.1",
    "subsetted-fonts": "^1.0.4",
    "sudo-prompt": "^9.2.1",
    "tippy.js": "^6.3.7",
    "uuid": "^11.1.0",
    "vite-plugin-pwa": "^1.0.0",
    "vue": "^3.5.16",
    "vue-i18n": "^11.1.5",
    "vue-json-pretty": "^2.4.0",
    "vue-router": "^4.5.1",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "3.0.0",
    "@electron-toolkit/eslint-config-ts": "^3.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@tailwindcss/postcss": "^4.1.10",
    "@tsconfig/node22": "^22.0.2",
    "@types/echarts": "^5.0.0",
    "@types/lodash": "^4.17.16",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.14.1",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitejs/plugin-vue-jsx": "^4.1.2",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/tsconfig": "^0.7.0",
    "electron": "^35.1.5",
    "electron-builder": "^25.1.8",
    "electron-vite": "^3.1.0",
    "eslint": "^9.24.0",
    "eslint-plugin-vue": "^10.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.0.0",
    "postcss": "^8.5.3",
    "postcss-conditionals": "^2.1.0",
    "postcss-for": "^2.1.1",
    "prettier": "^3.5.3",
    "tailwind-merge": "^3.3.0",
    "tailwindcss": "^4.1.7",
    "typescript": "^5.8.3",
    "vite": "^6.2.6",
    "vue-eslint-parser": "^10.1.3",
    "vue-tsc": "^2.2.8"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "electron",
      "esbuild"
    ]
  }
}